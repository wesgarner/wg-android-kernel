rm -r out
if [ ! -d out ]; then
	mkdir out;
fi;
mkbootfs ./ramdisk.donut | gzip > out/ramdisk.gz
cp -a arch/arm/boot/zImage out/
mkbootimg --kernel out/zImage --ramdisk out/ramdisk.gz -o out/boot.img
cp -a drivers/net/wireless/tiwlan1251/wlan.ko out/
cp -a compcache/ramzswap.ko out/
cp -a compcache/sub-projects/rzscontrol/rzscontrol out/
